<?xml version="1.0" encoding="utf-8" ?>
<Defs>  

  <ThingDef ParentName="NonDeconstructibleAncientBuildingBase">
    <defName>CormorantWreck</defName>
    <label>small cargo ship wreck</label>
    <description>text.todo.</description>
    <rotatable>false</rotatable>
    <graphicData>
      <texPath>Wrecks/CormorantWreck</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,7)</drawSize>
    </graphicData>
    <size>(4,7)</size>
    <fillPercent>0.5</fillPercent>
    <castEdgeShadows>false</castEdgeShadows>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <statBases>
      <MaxHitPoints>6000</MaxHitPoints>
	  <Flammability>0</Flammability>
    </statBases>
    <killedLeavings>
      <ChunkSlagSteel>16</ChunkSlagSteel>
    </killedLeavings>
	<comps>
		<li Class="VanillaVehiclesExpanded.CompProperties_VehicleWreck">
			<vehicleDef>Cormorant</vehicleDef>
			<spawnRotation>East</spawnRotation>
		</li>
	</comps>
  </ThingDef>

